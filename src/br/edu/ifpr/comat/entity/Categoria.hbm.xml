<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.edu.ifpr.comat.model.Categoria" table="Categoria"
		catalog="comat">
		<id name="idCategoria" type="java.lang.Integer">
			<column name="idCategoria" />
			<generator class="identity" />
		</id>
		<property name="nomeCategoria" type="java.lang.String">
			<column name="nomeCategoria" length="42" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
